<?xml version="1.0" encoding="UTF-8"?>
<build>
  <key name="name">Digital Generation</key>
  <key name="email">Dietrich Epp &lt;depp@zdome.net&gt;</key>
  <key name="url">http://moria.us/ludumdare/ld24/</key>
  <key name="copyright">Copyright Â© 2012 Dietrich Epp</key>
  <key name="version">1.0</key>
  <key name="apple-category">public.app-category.arcade-games</key>
  <key name="identifier">us.moria.diggen</key>

  <key name="cvar.log.level.root">debug</key>
  <if value="$os == windows">
    <key name="cvar.log.winconsole">yes</key>
  </if>
  <key name="cvar.path.data"><path path="data"/></key>
  <key name="cvar.path.user"><path path="user"/></key>

  <key name="lib-dir"><path path="lib"/></key>

  <module type="sglib++">
    <key name="icon.osx"><path path="resources/Icon.icns"/></key>
    <key name="icon.windows"><path path="resources/icon.ico"/></key>

    <group path="src">
      <src path="defs.hpp"/>
      <src path="gamescreen.cpp"/>
      <src path="gamescreen.hpp"/>
      <src path="lball.cpp"/>
      <src path="lball.hpp"/>
      <src path="lchase.cpp"/>
      <src path="lchase.hpp"/>
      <src path="level.cpp"/>
      <src path="level.hpp"/>
      <src path="linvade.cpp"/>
      <src path="linvade.hpp"/>
      <src path="ltitle.cpp"/>
      <src path="ltitle.hpp"/>
      <src path="main.cpp"/>
      <src path="sprite.cpp"/>
      <src path="sprite.hpp"/>
      <src path="text.cpp"/>
      <src path="text.hpp"/>
      <src path="zone.cpp"/>
      <src path="zone.hpp"/>
    </group>
  </module>
</build>
